<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.xiao.oasystem.mapper.RecordMapper">
    <insert id="insertRecord">
        insert into record values (#{userId},#{message},#{date})
    </insert>

    <select id="list" resultType="com.xiao.oasystem.pojo.entity.Record">
        select * from record
    </select>
</mapper>