<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.example.startingNovel.mapper.keywordMapper">

    <select id="getKeywordByKeywordId" resultType="com.example.startingNovel.pojo.keyword">
        select * from keyword_ where keywordId = ${keywordId}
    </select>

    <select id="getKeywordByContent" resultType="com.example.startingNovel.pojo.keyword">
        select * from keyword_ where content = '${content}'
    </select>

    <select id="getAllKeyword" resultType="com.example.startingNovel.pojo.keyword">
        select * from keyword_
    </select>

</mapper>